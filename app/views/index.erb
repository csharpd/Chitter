<div id='peeps-container'>
  <ul id='peeps'>
    <% @peeps.reverse.each do |peep| %>
       <li class='peep'>
        <div class='description'>
          <%= peep.content %>
        </div>
        <span class='source'>
          Added on <%= peep.created_at.strftime("%m/%d/%y at %I:%M%p") %> by <%= peep.user.name %> | @<%= peep.user.username %>
        </span>
      </li>
    <% end %>
  </ul>
</div>


